// @preview image


200 'width' STORE
200 'height' STORE

$width $height '2D3' PGraphics
# /home/pierre/Images/photoPP_low.jpg
'data:image/jpg;base64,'
Pdecode
0 0 Pimage
'source' STORE


$source
Pencode

// build a random number of control points
100 'nCtrlPoints' STORE

[] 'xy' STORE
[] 'luminosity' STORE 

$source Ppixels 'pixels' STORE DROP // get pixels  

1 $nCtrlPoints 
<%
  $width RAND * TOLONG  $width 1 - MIN 'x' STORE
  $height RAND * TOLONG $height 1 - MIN 'y' STORE
  $xy [ $x TODOUBLE $y TODOUBLE ] +!  DROP 

  $pixels $x $y $width * + GET 'color' STORE DROP 
  $source $color Pbrightness 'l' STORE DROP 
  $luminosity  $l TODOUBLE +! DROP 

%> FOR 

// draw controlpoints
$source
0 $nCtrlPoints 1 -
<%  
  'i' STORE
  'CENTER' PrectMode
  $xy $i GET LIST-> DROP  3 3 Prect
%> FOR 
Pencode

// interpolator
$xy $luminosity 
{
 "max.dark.fraction"  0.1
 "dark.threshold"     1e-3
 "background"         NaN 
 "exponent"           1.1
 "no.interpolation.tolerance"  0.001
 "elements" 10
}
 MICROSPHEREFIT 'interpolator' STORE

// try to interpolate
[] 'interpolatedPixels' STORE 
0 $height 1 -
<%
  'y' STORE
  0 $width 1 - 
  <%
    'x' STORE
    [ $x TODOUBLE $y TODOUBLE ] $interpolator EVAL 'l' STORE
    $source $l Pcolor 'color' STORE DROP 
    $interpolatedPixels $color  +! DROP  
  %> FOR 
%> FOR 

$width $height '2D3' PGraphics
$interpolatedPixels PupdatePixels
Pencode